<?xml version="1.0" encoding="UTF-8"?>
<localEntry key="WorkForceAPI" xmlns="http://ws.apache.org/ns/synapse">
  <http.init>
    <connectionType>HTTP</connectionType>
    <baseUrl>http://localhost:3000</baseUrl>
    <authType>None</authType>
    <timeoutDuration>4000</timeoutDuration>
    <timeoutAction>Never</timeoutAction>
    <retryCount>3</retryCount>
    <retryDelay>5000</retryDelay>
    <suspendErrorCodes>500</suspendErrorCodes>
    <suspendInitialDuration>-1</suspendInitialDuration>
    <suspendProgressionFactor>1</suspendProgressionFactor>
    <name>WorkForceAPI</name>
  </http.init>
</localEntry>